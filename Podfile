# platform :ios, '9.0'

target 'Tuto Learn' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
   pod 'IQKeyboardManagerSwift'
   pod 'Alamofire'
   pod 'FacebookCore'
   pod 'FacebookLogin'
   pod 'FacebookShare'
   pod 'Google/SignIn'
   pod 'SideMenu'
   pod 'MBProgressHUD'
  # Pods for Tuto Learn

  target 'Tuto LearnTests' do
    inherit! :search_paths
   pod 'IQKeyboardManagerSwift'
   pod 'Alamofire'
   pod 'FBSDKLoginKit'
   pod 'FacebookCore'
   pod 'FacebookLogin'
   pod 'FacebookShare'
   pod 'Google/SignIn'
   pod 'SideMenu'
   pod 'MBProgressHUD'
    # Pods for testing
  end

  target 'Tuto LearnUITests' do
    inherit! :search_paths
   pod 'IQKeyboardManagerSwift'
   pod 'Alamofire'
   pod 'FBSDKLoginKit'
   pod 'FacebookCore'
   pod 'FacebookLogin'
   pod 'FacebookShare'
   pod 'Google/SignIn'
   pod 'SideMenu'
   pod 'MBProgressHUD'
    # Pods for testing
  end

end
